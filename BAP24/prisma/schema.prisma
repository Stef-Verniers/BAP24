// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = "mysql://root:root@127.0.0.1:3306/bap"
}

model User {
  id       Int    @id @default(autoincrement())
  username String
  email    String @unique
  password String
}
